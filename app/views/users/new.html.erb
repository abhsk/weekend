<%= form_for @user, url: users_path, method: :post do |f| %>
    <div>
      <%= f.label :name, "Name" %>
      <%= f.text_field :name %>
    </div>
    <div>
      <%= f.label :email, "Email" %>
      <%= f.text_field :email %>
    </div>
    <div>
      <%= submit_tag "Submit" %>
    </div>
<%- end %>
